<service>
	<id>webdesktop.exe</id>
	<name>Web Desktop</name>
	<description>ZQ 的 Web 桌面系统服务</description>
	<executable>D:\soft\node-v12.10.0-win-x64\node.exe</executable>
	<argument>--harmony</argument>
	<argument>E:\MyGit\webdesktop\node_modules\node-windows\lib\wrapper.js</argument>
	<argument>--file</argument>
	<argument>E:\MyGit\webdesktop\index.js</argument>
	<argument>--scriptoptions=savepid</argument>
